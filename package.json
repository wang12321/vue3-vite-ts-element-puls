{
  "name": "vue3-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:npm": "vue-tsc --noEmit && vite build --mode productionNpm",
    "serve": "vite preview",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx,js,json,vue}\"",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,ts,css,scss,vue,html,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "axios": "^0.24.0",
    "element-plus": "^1.1.0-beta.18",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.19.1",
    "fast-glob": "^3.2.7",
    "js-cookie": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.4.1",
    "screenfull": "^5.1.0",
    "vite-plugin-style-import": "^1.2.1",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "~11.0.0",
    "@commitlint/config-conventional": "~11.0.0",
    "@types/node": "^16.10.2",
    "@types/prettier": "^2.4.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.2",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "husky": "^7.0.2",
    "sass": "^1.42.1",
    "typescript": "^4.4.3",
    "unplugin-vue-components": "^0.15.4",
    "vite": "^2.6.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^1.0.5",
    "vite-plugin-vue-docs": "^0.1.15",
    "vue-tsc": "^0.3.0"
  }
}
